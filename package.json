{
  "name": "juttle-opentsdb-adapter",
  "version": "0.1.2",
  "description": "OpenTSDB Adapter for Juttle",
  "keywords": [
    "juttle",
    "adapter",
    "opentsdb"
  ],
  "homepage": "https://github.com/juttle/juttle-opentsdb-adapter",
